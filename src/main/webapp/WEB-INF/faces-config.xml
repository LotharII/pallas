<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

    <navigation-rule>
        <from-view-id>/pages/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>personDashboard</from-outcome>
            <to-view-id>/pages/dashboards/person-dashboard.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>hoursType</from-outcome>
            <to-view-id>/pages/dashboards/hours-type.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>links</from-outcome>
            <to-view-id>/pages/dashboards/link-dashboard.xhtml</to-view-id>
            <redirect/>
        </navigation-case>

    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/dashboards/person-dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>overview</from-outcome>
            <to-view-id>/pages/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/pages/dashboards/hours-type.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>overview</from-outcome>
            <to-view-id>/pages/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/pages/dashboards/link-dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>overview</from-outcome>
            <to-view-id>/pages/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>results</from-outcome>
            <to-view-id>/pages/dashboards/link-result-dashboard.xhtml</to-view-id>
            <redirect/>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/pages/dashboards/link-result-dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>overview</from-outcome>
            <to-view-id>/pages/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>links</from-outcome>
            <to-view-id>/pages/dashboards/link-dashboard.xhtml</to-view-id>
            <redirect/>
        </navigation-case>

    </navigation-rule>
</faces-config>